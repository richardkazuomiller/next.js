{
  "extends": [
    "config:base"
  ],
  "dependencyDashboard": true,
  "prHourlyLimit": 0,
  "baseBranches": ["canary", "next"],
  "gitAuthor": "Ricky Miller <ricky@ricky.me>",
  "rangeStrategy": "bump",
  "ignorePaths": [
    "packages/next/compiled/**/*"
  ],
  "packageRules": [
    {
      "enabled": false
    },
    {
      "matchBaseBranches": ["canary"],
      "rangeStrategy": "replace"
    },
    {
      "matchPackagePrefixes": ["eslint-"],
      "enabled": true
    }
  ]
}
